<?xml version="1.0"?>
    <YnoteSyntax>
        <Language CommentPrefix="//" Extensions=".sl"/>
        <Brackets Left="(" Right=")"/>
        <Rule Type="Comment" Options="Multiline" Regex="//.*$"></Rule>
        <Rule Type="String" Options="Multiline" Regex="&quot;&quot;|''|&quot;.*?[^\\]&quot;|'.*?[^\\]'"></Rule>
        <Rule Type="Keyword" Regex="\b(class|my|public|for|foreach|var|use|shared|if|else|return|try|catch|end)\b"></Rule>
        <Rule Type="Keyword2" Regex="\b(function|true|false|int|byte|string|ushort|char|null)\b"></Rule>
        <Rule Type="Preprocessor" Options="Multiline" Regex="#.*$"></Rule>
        <Folding Start=":" End="end" Options="None"></Folding>
    </YnoteSyntax>