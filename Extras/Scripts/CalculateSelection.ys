using NCalc;
using System.Windows.Forms;
using SS.Ynote.Classic.UI;

public static void Run(IYnote ynote)
{
    var self = ynote.Panel.ActiveDocument as Editor;
    Expression e = new Expression(self.tb.SelectedText);
    if(e != null)
        self.tb.SelectedText = e.Evaluate().ToString();
}
